package Megagfg;

import java.util.Scanner;

public class Occurenceofelement  {
	void solution(int []nums,int n,int k)
	{
		int i;
		//int max = Integer.MIN_VALUE;
		//int l;
		//l=k;
		for(i=0;i<n;i++)
		{
			if(nums[i] == k)
			{
		     System.out.print(i);
		     break;
			}
			
	}
		
	}
	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
		Occurenceofelement call = new Occurenceofelement();
		Scanner sc = new Scanner(System.in);
		
		int []nums = {2,8,3,9,6,5,4};
        int n = nums.length;
        int k =sc.nextInt();
		call.solution(nums,n,k);

}
}
