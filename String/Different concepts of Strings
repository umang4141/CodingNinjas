import java.io.*;
import java.util.*;
import java.util.ArrayList;
class HelloWorld {
    public static void main(String[] args) {
       String ans = "";
       for(int i=0;i<26;i++)
       {
           char ch = (char)('a' + i);
           ans = ans + ch;
       }
       System.out.println(ans);
        
    }
}
